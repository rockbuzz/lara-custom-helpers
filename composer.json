{
  "name": "rockbuzz/lara-custom-helpers",
  "description": "laravel custom helpers",
  "keywords": ["laravel", "helpers"],
  "license": "MIT",
  "authors": [
    {
      "name": "TiagoDevWeb",
      "email": "tiago@rockbuzz.com.br"
    },
    {
      "name": "Paulo Henrique P. Scheuermann",
      "email": "paulo@rockbuzz.com.br"
    }
  ],
  "autoload": {
    "psr-4": {
        "Rockbuzz\\LaraCustomHelpers\\": "src/"
    },
    "files": [
        "src/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "require": {
    "php": ">=7.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0",
      "orchestra/testbench": "^4.0",
      "squizlabs/php_codesniffer": "^3.2"
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "cs": "vendor/bin/phpcs"
  }
}
